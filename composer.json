{
    "name": "almirhodzic/nova-toggle",
    "type": "project",
    "version": "1.0.0",
    "description": "A Laravel Nova 5 toggle field for quick boolean updates on index view",
    "keywords": [
        "laravel",
        "nova",
        "toggle",
        "field",
        "boolean"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "./packages/nova-toggle",
            "options": {
                "symlink": true
            }
        }
    ],
    "authors": [
        {
            "name": "Almir Hodzic",
            "email": "hello@frontbyte.ch"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/nova": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "AlmirHodzic\\NovaToggle\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlmirHodzic\\NovaToggle\\FieldServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
